# preprocessing
ifdef                   /ifdef/
ifndef                  /ifndef/
elif                    /elif/
endif                   /endif/
include                 /include/
define                  /define/
undef                   /undef/
line                    /line/
error                   /error/
pragma                  /pragma/

# identifier
identifier              /{alpha_}{alnum_}+/

# number constants
decimal_constant        /[1-9][0-9]*/
octal_constant          /0[0-7]*/
hexadecimal_constant    /(0(x|X))[0-9a-fA-F]+/

# char constant
char_sequence           /'(\\.|[^'])'/
string_sequence         /"(\\.|[^"])*"/

# header_name             /<[^\n>]>|"[^\n"]"/
# pp_number               /([0-9]|\.[0-9])(([0-9]|{alpha_})|(e|E|p|P)(\+|-))*/
# lparen                  /[^\t\n\v\f\r ]\(/
# new_line                /\n/
